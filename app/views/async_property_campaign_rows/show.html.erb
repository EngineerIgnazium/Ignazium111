<%= table_row do %>
  <%= table_data @campaign.name %>
  <% unless @property.restrict_to_sponsor_campaigns? %>
    <%= table_data number_with_delimiter(@property.campaign_impressions_count(@campaign, @start_date, @end_date)) %>
  <% end %>
  <%= table_data number_with_delimiter(@property.campaign_clicks_count(@campaign, @start_date, @end_date)) %>
  <% unless @property.restrict_to_sponsor_campaigns? %>
    <%= table_data number_to_percentage(@property.campaign_click_rate(@campaign, @start_date, @end_date), precision: 2) %>
    <%= table_data(@property.campaign_property_revenue(@campaign, @start_date, @end_date).format) %>
  <% end %>
</tr>
