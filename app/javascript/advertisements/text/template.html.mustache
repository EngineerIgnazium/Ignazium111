<div id="cf" data-template="text" data-theme="{{theme}}">
  <b>{{creative.headline}}</b>
  {{creative.body}}
  <a href="{{urls.campaign}}" class="cf-cta" target="_blank" rel="sponsored noopener">
    <b>{{creative.cta}}</b>
  </a>
  <img src="{{urls.instantImpression}}" data-src="{{urls.impression}}" data-behavior="trackImpression" class="cf-impression" alt="">
</div>
